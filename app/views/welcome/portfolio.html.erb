<div class="container-fluid portfolio-cont">
  <header>
    <h1 class="port-header">Portfolio</h1>
  </header>
  <nav class="port-nav">
    <a href="#" id="real-estate-link" class="port-links">Real Estate</a>
    <a href="#" id="headshot-link" class="port-links">Headshots</a>
    <a href="#" id="Landscape-link" class="port-links">Landscape</a>
    <a href="#" id="Cityscape-link" class="port-links">Cityscape</a>
  </nav>
  <main>
    <div id="modal-container">
      <a id="port-prev">prev</a>
      <%= image_tag("", :alt => "Britt", :id => "modal-content") %>
      <a id="port-next">next</a>
    </div>
    <section>
      <div class="port-row">
        <section class="real-estate-port port-page active">
          <div class="port-column">
            <%= image_tag("bath.jpg", :alt => "Modern bathroom", :class => "img-real", :data => {:number => 0}) %>
            <%= image_tag("bar.jpg", :alt => "Bar", :class => "img-real", :data => {:number => 1}) %>
            <%= image_tag("living.jpg", :alt => "Livingroom", :class => "img-real", :data => {:number => 2}) %>
          </div>
          <div class="port-column">
            <%= image_tag("bed-cup.jpg", :alt => "Kitchen with sitting area", :class => "img-real",:data => {:number => 3}) %>
            <%= image_tag("bed.jpg", :alt => "Modern bedroom", :class => "img-real", :data => {:number => 4}) %>
            <%= image_tag("cabin-living.jpg", :alt => "Kitchen showing appliances", :class => "img-real", :data => {:number => 5}) %>
          </div>
          <div class="port-column">
            <%= image_tag("cabin.jpg", :alt => "Master bed", :class => "img-real", :data => {:number => 6}) %>
            <%= image_tag("Kitchen-in.jpg", :alt => "Britt", :class => "img-real", :data => {:number => 7}) %>
            <%= image_tag("Kitchen-out.jpg", :alt => "Britt", :class => "img-real", :data => {:number => 8}) %>
          </div>
          <div class="port-column">
            <%= image_tag("small-bath.jpg", :alt => "Kitchen showing appliances", :class => "img-real", :data => {:number => 9}) %>
            <%= image_tag("Kitchen-wide.jpg", :alt => "Kitchen with sitting area", :class => "img-real",:data => {:number => 10}) %>
            <%= image_tag("m-kitchen.jpg", :alt => "Modern bedroom", :class => "img-real", :data => {:number => 11}) %>
          </div>
        </section>

        <section class="headshot-port port-page">
          <div class="port-column">

          </div>
          <div class="port-column">

          </div>
          <div class="port-column">

          </div>
          <div class="port-column">

          </div>
        </section>
      </div>
    </section>

  </main>
</div>

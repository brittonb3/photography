        <%= image_tag("master_1.jpg", :alt => "Modern bedroom", :class => "port-img img-real", :data => {:number => 4}) %>
        <%= image_tag("kitchen_2.jpg", :alt => "Kitchen showing appliances", :class => "port-img img-real", :data => {:number => 5}) %>
      </div>
      <div class="port-column">
        <%= image_tag("master_2.jpg", :alt => "Master bed", :class => "port-img img-real", :data => {:number => 6}) %>

<section class="container-fluid">
  <div class="row">
    <div class="col-xs-12 text-center port-links-div portfolio-color">
      <h1 id="portfolio-head">Portfolio</h1>
      <ul class="port-links">
        <li class="port-link"><a href="">Landscape / Cityscape</a></li>
        <li class="port-link"><a href="">Headshots</a></li>
        <li class="port-link"><a href="">Real Estate</a></li>
        <li class="port-link"><a href="">Portraits</a></li>
      </ul>
    </div>
    <div class="col-xs-12 portfolio-color" id="image-div">
      <%= image_tag("Split_rock.jpg", :alt => "Split rock lighthouse, MN", :class => "col-xs-12 col-sm-4 port-image ") %>
      <%= image_tag("dark_mountain.jpg", :alt => "Alaska", :class => "col-xs-12 col-sm-4 port-image") %>
      <%= image_tag("Skagway_finished.jpg", :alt => "Skagway, AK", :class => "col-xs-12 col-sm-4 port-image") %>
      <%= image_tag("MSP_finished-2.jpg", :alt => "MSP", :class => "col-xs-12 col-sm-4 port-image") %>
      <%= image_tag("Montana_finished.jpg", :alt => "Montana", :class => "col-xs-12 col-sm-4 port-image") %>
      <%= image_tag("Seattle.jpg", :alt => "Seattle", :class => "col-xs-12 col-sm-4 port-image") %>
    </div>
  </div>
</section>

